{
  "name": "name_hub",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run build:web",
    "build:contract": "../contract/build.sh",
    "build:contract:dev": "../contract/build.sh",
    "build:web": "mkdir -p dist && cp CNAME dist/CNAME && parcel build src/index.html --public-url ./",
    "deploy:contract:dev": "npm run build:contract:dev && near dev-deploy --wasmFile ../contract/res/marketplace.wasm --initFunction new --initArgs '{\"seller_rewards_commission\":{\"num\":1,\"denom\":10},\"bid_step\":{\"num\":1,\"denom\":5},\"prev_bidder_commission_share\":{\"num\":1,\"denom\":2}}'",
    "deploy:contract": "npm run build:contract && near deploy --wasmFile ../contract/res/marketplace.wasm --initFunction new --initArgs '{\"seller_rewards_commission\":{\"num\":1,\"denom\":10},\"bid_step\":{\"num\":1,\"denom\":5},\"prev_bidder_commission_share\":{\"num\":1,\"denom\":2}}'",
    "deploy:pages": "npm run build && gh-pages -d dist/",
    "deploy": "npm run deploy:pages",
    "start": "echo The app is starting! It will automatically open in your browser when ready && env-cmd -f ./neardev/dev-account.env parcel src/index.html --open",
    "dev": "nodemon --watch ../contract/src -e rs --exec \"npm run start\"",
    "test": "jest test --runInBand"
  },
  "devDependencies": {
    "@babel/core": "~7.14.0",
    "@babel/preset-env": "~7.14.0",
    "@babel/preset-react": "~7.13.13",
    "babel-jest": "~26.6.2",
    "env-cmd": "~10.1.0",
    "gh-pages": "~3.1.0",
    "jest": "~26.6.2",
    "jest-environment-node": "~26.6.2",
    "near-cli": "~3.1.0",
    "nodemon": "~2.0.3",
    "parcel-bundler": "~1.12.4",
    "react-test-renderer": "~17.0.1",
    "sass": "^1.43.4",
    "shelljs": "~0.8.4",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.0.0"
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@types/react": "^16.8.6 || ^17.0.0",
    "big.js": "^6.1.1",
    "bootstrap": "5.1.3",
    "local-storage": "^2.0.0",
    "near-api-js": "~0.43.1",
    "near-seed-phrase": "^0.2.0",
    "react": "^17.0.2",
    "react-countdown": "^2.3.2",
    "react-dom": ">=16.8",
    "react-is": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "~0.13.5",
    "swr": "^1.1.0"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/src/jest.init.js"
    ],
    "testEnvironment": "near-cli/test_environment",
    "testPathIgnorePatterns": [
      "<rootDir>/contract/",
      "<rootDir>/node_modules/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
